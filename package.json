{
  "name": "reduxstagram",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts-ts": "2.7.0"
  },
  "scripts": {
    "build:styl": "stylus src/styles/style.styl -o src/styles/style.css",
    "watch:styl": "npm run build:styl && stylus -w src/styles/style.styl -o src/styles/style.css",
    "start:ts": "react-scripts-ts start",
    "start": "npm-run-all -p watch:styl start:ts",
    "build": "npm run build:styl && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^21.1.0",
    "@types/node": "^8.0.30",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "npm-run-all": "^4.1.1",
    "stylus": "^0.54.5"
  }
}
