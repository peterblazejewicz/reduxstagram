{
  "name": "reduxstagram",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "raven-js": "^3.17.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.7.0",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-extension": "^2.13.2"
  },
  "scripts": {
    "build:styl": "stylus src/styles/style.styl -o src/styles/style.css",
    "watch:styl": "npm run build:styl && stylus -w src/styles/style.styl -o src/styles/style.css",
    "start:ts": "react-scripts-ts start",
    "start": "npm-run-all -p watch:styl start:ts",
    "build": "npm run build:styl && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^21.1.0",
    "@types/node": "^8.0.30",
    "@types/raven": "^2.1.2",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-redux": "^5.0.9",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-transition-group": "^2.0.5",
    "@types/redux": "^3.6.0",
    "@types/redux-devtools": "^3.0.40",
    "@types/redux-devtools-extension": "^2.13.2",
    "npm-run-all": "^4.1.1",
    "stylus": "^0.54.5"
  }
}
